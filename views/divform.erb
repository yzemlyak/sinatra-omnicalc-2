<h1>New division</h1>

<form action="/wizard_divide">
  <label for="numerator">
    Divide this:
  </label>
  <input id="numerator" name="first_num" text="text">

  <label for="denominator">
    by this:
  </label>
  <input id="denominator" name="second_num" text="text">

  <button>
    Divide!
  </button>
</form>
